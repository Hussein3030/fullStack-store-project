WAAA,EAAA,KAAA,CAAA,WAFA;AAGA,kBAAA,KAAA,EAAA,KAAA,CAAA,KAHA;AAIA,kBAAA,aAAA,EAAA,KAAA,CAAA;AAJA,iBAAA,CAHA;;AAAA;AAGA,gBAAA,QAHA;AASA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,2BAAA,QAAA,CAAA,IAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,kBAAA,KAAA,CAAA,WAAA,GAAA,gBAAA,GAAA,KAAA,CAAA,WAAA,GAAA,UAAA,GAAA,KAAA,CAAA,KAAA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KAlBA;AAmBA,IAAA,QAnBA,sBAmBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,gBAAA,QAFA,GAEA,IAAA,QAAA,EAFA;AAGA,gBAAA,QAAA,CAAA,MAAA,CAAA,MAAA,EAAA,MAAA,CAAA,IAAA,EAAA,MAAA,CAAA,aAAA,CAAA,IAAA,EAHA,CAGA;;AAHA;AAAA,uBAIA,KAAA,CAAA,IAAA,CAAA,8BAAA,EAAA,QAAA,CAJA;;AAAA;AAIA;AACA,gBAAA,MAAA,CAAA,OAAA,GAAA,4BAAA;AACA,gBAAA,MAAA,CAAA,IAAA,GAAA,EAAA,CANA,CAMA;;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,mBAAA;AAPA;AAAA;;AAAA;AAAA;AAAA;AASA,gBAAA,MAAA,CAAA,OAAA,GAAA,sBAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,uCAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA;AA/BA;AAdA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"container\" style=\"padding-top: 10%\">\r\n      <div class=\"row d-flex justify-content-center\">\r\n        <div class=\"col-5 text-left login-form-container\">\r\n          <div class=\"d-flex justify-content-center\">\r\n            <label><strong>Add product to the online store</strong></label>\r\n            <br>\r\n          </div>\r\n          <div>\r\n            <div class=\"form-group\">\r\n              <label for=\"exampleInputEmail1\">Name of the product</label>\r\n              <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"exampleInputEmail1\"\r\n                  aria-describedby=\"emailHelp\"\r\n                  placeholder=\"add the product name\"\r\n                  v-model='nameProduct'\r\n              />\r\n\r\n            </div>\r\n            <div class=\"form-group\">\r\n              <label for=\"exampleInputPassword1\">description</label>\r\n              <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"exampleInputPassword1\"\r\n                  placeholder=\"Describe the product\"\r\n                  v-model='description'\r\n              />\r\n            </div>\r\n\r\n            <div class=\"form-group\">\r\n              <label for=\"exampleInputPassword1\">Price €</label>\r\n              <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"exampleInputPassword1\"\r\n                  placeholder=\"product Price €\"\r\n                  v-model='price'\r\n              />\r\n            </div>\r\n\r\n            <div class=\"form-group\">\r\n              <label>Add an img</label>\r\n              <br>\r\n              <input type=\"file\" @change=\"selectedFile\" ref=\"file\"/>\r\n              <br>\r\n              <button class=\"btn btn-primary btn-block\" @click=\"sendFile\">Upload</button>\r\n              <br>\r\n              <p v-if=\"message\">{{message}}</p>\r\n            </div>\r\n\r\n            <div class=\"form-group form-check\">\r\n              <label class=\"form-check-label\" for=\"exampleCheck1\"></label>\r\n            </div>\r\n            <button @click=\"addProduct\" class=\"btn btn-primary btn-block\">Submit</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AuthenticationService from '../../../services/AuthenticationService';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'AddProduct',\r\n\r\n  data() {\r\n    return {\r\n      nameProduct: '',\r\n      description: '',\r\n      price: '',\r\n      selectedField: '',\r\n      file: '',\r\n      message: '',\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    selectedFile() {\r\n      this.file = this.$refs.file.files[0];\r\n      this.message = '';\r\n    },\r\n\r\n    async addProduct() {\r\n      let fd = new FormData();\r\n      fd.append('selectedField', this.selectedField, this.selectedField.name);\r\n      const response = await AuthenticationService.addProduct({\r\n        nameProduct: this.nameProduct,\r\n        description: this.description,\r\n        price: this.price,\r\n        selectedField: this.selectedField,\r\n      });\r\n      //console.log('fd: ' + fd)\r\n      console.log('The response data is: ' + response.data);\r\n      console.log('nameProduct: ' + this.nameProduct + ' description: ' + this.description + ' price: ' + this.price);\r\n    },\r\n    async sendFile() {\r\n      try {\r\n        const formData = new FormData();\r\n        formData.append('file', this.file,this.selectedField.name); // , this.selectedField.name\r\n        await axios.post('http://localhost:8000/Upload', formData);// it is necessary to add the hole url\r\n        this.message = 'File uploaded successfully';\r\n        this.file = ''; // to reset Upload\r\n        console.log('Upload successful');\r\n      } catch (e) {\r\n        this.message = 'Something went wrong';\r\n        console.log('Something went wrong: ' + e);\r\n      }\r\n    },\r\n  },\r\n\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.form-control {\r\n  border-radius: 0%;\r\n  height: 50px;\r\n}\r\n\r\n.login-form-container {\r\n  padding: 20px;\r\n  box-shadow: 0px 2px 5px 2px #888888;\r\n}\r\n\r\n.btn {\r\n  border-radius: 0%;\r\n  font-weight: bold;\r\n  background: #2c3e50;\r\n  border: #2c3e50;\r\n}\r\n\r\n.btn:hover {\r\n  background: #2c3e50;\r\n}\r\n\r\n.btn:focus {\r\n  background: #2c3e50;\r\n}\r\n</style>"],"sourceRoot":"src/views/home"}]}