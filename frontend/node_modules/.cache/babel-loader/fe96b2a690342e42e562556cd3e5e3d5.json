\n            <div class=\"form-group\">\r\n              <label for=\"exampleInputPassword1\">Price €</label>\r\n              <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"exampleInputPassword1\"\r\n                  placeholder=\"product Price €\"\r\n                  v-model='price'\r\n              />\r\n            </div>\r\n\r\n            <div class=\"form-group\">\r\n            <label>Add an img</label>\r\n              <br>\r\n              <input type=\"file\" @change=\"onFileSelected\" ref=\"file\">\r\n              <br>\r\n              <button @click=\"onUpload\">Upload</button>\r\n            </div>\r\n\r\n            <div class=\"form-group form-check\">\r\n              <label class=\"form-check-label\" for=\"exampleCheck1\"></label>\r\n            </div>\r\n            <button @click=\"addProduct\" type=\"submit\" class=\"btn btn-primary btn-block\">Submit</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AuthenticationService from '../../../services/AuthenticationService';\r\nimport axios from 'axios';\r\nexport default {\r\n  name: 'AddProduct',\r\n\r\n  data() {\r\n    return {\r\n      nameProduct: '',\r\n      description: '',\r\n      price:'',\r\n      selectedField: '',\r\n\r\n    };\r\n  },\r\n\r\n  methods:{\r\n    onFileSelected(){\r\n    this.selectedField = this.$refs.file.files[0];\r\n    },\r\n    async addProduct(){\r\n      const fd = new FormData();\r\n      fd.append('image', this.selectedField, this.selectedField.name);\r\n      const response = await AuthenticationService.addProduct({\r\n        nameProduct: this.nameProduct,\r\n        description: this.description,\r\n        price: this.price,\r\n       // selectedField: this.selectedField,\r\n      });\r\n      //console.log('fd: ' + fd)\r\n      console.log('The response data is: '+ response.data);\r\n      console.log('nameProduct: ' + this.nameProduct + ' description: ' + this.description + ' price: ' + this.price);\r\n    },\r\n    async onUpload(){\r\n      const fd = new FormData();\r\n      fd.append('selectedField', this.selectedField); // , this.selectedField.name\r\n      try {\r\n        await axios.post('/Upload', fd)\r\n      }catch (e) {\r\n        console.log(e);\r\n      }\r\n    },\r\n  },\r\n\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.form-control {\r\n  border-radius: 0%;\r\n  height: 50px;\r\n}\r\n.login-form-container {\r\n  padding: 20px;\r\n  box-shadow: 0px 2px 5px 2px #888888;\r\n}\r\n.btn {\r\n  border-radius: 0%;\r\n  font-weight: bold;\r\n  background: #2c3e50;\r\n  border: #2c3e50;\r\n}\r\n.btn:hover {\r\n  background: #2c3e50;\r\n}\r\n.btn:focus {\r\n  background: #2c3e50;\r\n}\r\n</style>"],"sourceRoot":"src/views/home"}]}